<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!--Redis 服务端地址-->
  <add key="RedisServerIP" value="127.0.0.1:6379"/>
  <!--Redis 连接池大小-->
  <add key="RedisPoolSize" value="30"/>
  <!--Redis 连接池超时时长-->
  <add key="RedisPoolTimeOutSeconds" value="5"/>
  <!--Memcached 服务端地址-->
  <add key="MemcachedServerIP" value="127.0.0.1:6379"/>
</appSettings>
